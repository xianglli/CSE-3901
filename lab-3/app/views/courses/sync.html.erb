<body class="is-loading">

<!-- Wrapper -->
<div id="wrapper">

  <!-- Main -->
  <section id="main">
    <header>
      <h1>Sync with OSU success</h1>
    </header>
    <footer>
      <ul class="actions special">
          <li>
            <%= link_to 'Back to Courses list', courses_path %>
          </li>
      </ul>
    </footer>
  </section>

  <!-- Footer -->
  <footer id="footer">
    <ul class="copyright">
      <li>&copy; Future Tech Corp</li>
      <li>Idea: <a href="http://html5up.net">HTML5 UP</a></li>
