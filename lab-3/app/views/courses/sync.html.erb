<head>
  <title>Login</title>
  <meta charset="utf-8"/>
  <meta name="viewport" content="width=device-width, initial-scale=1"/>
  <!--[if lte IE 8]>
  <script src="./app/assets/js/html5shiv.js"></script><![endif]-->
  <%= stylesheet_link_tag 'main', media: 'all',
                          'data-turbolinks-track' => true %>
  <!--[if lte IE 9]>
  <link rel="stylesheet" href="./app/assets/css/ie9.css"/><![endif]-->
  <!--[if lte IE 8]>
  <link rel="stylesheet" href="./app/assets/css/ie8.css"/><![endif]-->
  <noscript>
    <link rel="stylesheet" href="app/assets/stylesheets/noscript.css"/>
  </noscript>
